<div class="dropdown">
    <a class="nav-link text-light" dropdown-toggle" role="button"
    id="dropdownMenuLink" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="font-size:14px">
        Lanches
    </a>

    <ul class="dropdown-menu" aria-labelledby="dropdownMenuLink">
        @foreach(var categoria in Model)
        {
            <li>
                <a class="dropdown-item" asp-controller="Lanche" asp-action="List" style="color:black"
               asp-route-categoria="@categoria.CategoriaNome">@categoria.CategoriaNome</a>
            </li>
        }
        <li class="dropdown-divider"></li>
        <li>
            <a class="dropdown-item" asp-controller="Lanche" asp-action="List" asp-route-categoria="" style="color:black">Todos os Lanches</a>
        </li>
    </ul>
</div>